generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id              String  @id @default(uuid())
  firstName       String  @map("first_name")
  lastName        String  @map("last_name")
  profileiamgeurl String? @map("profile_img_url")
  email           String  @unique
  password        String
  salt            String

  @@map("users")
}
